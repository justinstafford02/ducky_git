    "def exponents(base, power):\n",
    "    new_list = []\n",
    "    for i in base:\n",
    "        for j in power:\n",
    "            new_list.append(i ** j)\n",
    "    return new_list\n",
    "print(exponents([2, 3, 4], [1, 2, 3]))\n",
    "\n"
   ]
  }
